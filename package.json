{
  "name": "supernova",
  "version": "0.0.1",
  "description": "Electron + React + Redux + TypeScript + Webpack + TypeStyle Boilerplate",
  "main": "./app/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron ./app/main.js",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod",
    "dev-main": "webpack --watch --config ./webpack/target/main",
    "dev-renderer": "webpack-dev-server --config ./webpack/target/renderer",
    "dev": "concurrently \"npm run dev-renderer\" \"npm run dev-main\""
  },
  "repository": "git@github.com:Black-Monolith/SuperNova",
  "author": "Chris Feijoo <christophe@feijoo.fr>",
  "license": "MIT",
  "dependencies": {
    "@types/bluebird": "^3.5.7",
    "@types/express": "^4.0.36",
    "awesome-typescript-loader": "^3.1.3",
    "express": "^4.15.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "webpack-dev-server": "2.2.0"
  },
  "devDependencies": {
    "@blueprintjs/core": "^1.21.0",
    "@blueprintjs/labs": "^0.2.0",
    "@types/electron": "^1.4.31",
    "@types/es6-promise": "^0.0.32",
    "@types/react": "^15.0.31",
    "@types/react-dom": "^15.5.1",
    "@types/react-hot-loader": "^3.0.1",
    "@types/react-router": "^4.0.12",
    "@types/webpack": "^2.2.2",
    "@types/webpack-env": "^1.13.0",
    "@types/webpack-merge": "^0.0.4",
    "bluebird": "^3.5.0",
    "brace": "^0.10.0",
    "clean-webpack-plugin": "^0.1.15",
    "concurrently": "^3.1.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.28.4",
    "electron": "^1.4.15",
    "electron-debug": "^1.2.0",
    "file-loader": "^0.10.0",
    "glamor": "^2.20.25",
    "html-webpack-plugin": "^2.28.0",
    "react-ace": "^5.0.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-router": "^4.1.1",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.0.0",
    "typescript": "^2.3.4",
    "url-loader": "^0.5.9",
    "webpack": "2.2.0",
    "webpack-merge": "^2.5.0"
  }
}
